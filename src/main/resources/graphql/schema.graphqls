type Meta{
    id: ID!
    uuid: ID!
    descricao: String!
    valorObjetivo: Float!
    valorAtual: Float!
    dataCriacao: String!
    percentual: Float!
    concluida: Boolean!
}

type Query{
    metas: [Meta]
    metaPorUuid(uuid: ID!): Meta
}

type Mutation{
    criarMeta(descricao: String!, valorObjetivo: Float!): Meta
    atualizarMeta(uuid: ID!, valorEntrada: Float!): Meta
}